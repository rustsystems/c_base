input-language C/C++
decl-version 2.0
var-comparability none

ppt ..main():::ENTER
  ppt-type enter

ppt ..main():::EXIT0
  ppt-type subexit
  variable return
    var-kind variable
    rep-type int
    dec-type int

ppt ..makepoint():::ENTER
  ppt-type enter
  variable x
    var-kind variable
    rep-type int
    dec-type long
    flags is_param 
  variable y
    var-kind variable
    rep-type int
    dec-type long
    flags is_param 

ppt ..makepoint():::EXIT0
  ppt-type subexit
  variable return
    var-kind variable
    rep-type hashcode
    dec-type point
  variable return.x
    var-kind field x
    enclosing-var return
    rep-type int
    dec-type long
  variable return.y
    var-kind field y
    enclosing-var return
    rep-type int
    dec-type long
  variable x
    var-kind variable
    rep-type int
    dec-type long
    flags is_param 
  variable y
    var-kind variable
    rep-type int
    dec-type long
    flags is_param 

ppt ..writeCrap():::ENTER
  ppt-type enter
  variable s
    var-kind variable
    rep-type string
    dec-type char*
    flags is_param 

ppt ..writeCrap():::EXIT0
  ppt-type subexit
  variable s
    var-kind variable
    rep-type string
    dec-type char*
    flags is_param 

